      </ImageView>
      <FlowPane id="ContentPane" fx:id="contentPane" minHeight="50.0" minWidth="50.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </FlowPane>
   </children>
